## Perl5 local::lib
export PERL_LOCAL_LIB_ROOT="$PERL_LOCAL_LIB_ROOT:/home/holznera/perl5"
export PERL_MB_OPT="--install_base /home/holznera/perl5"
export PERL_MM_OPT="INSTALL_BASE=/home/holznera/perl5"
export PERL5LIB="/home/holznera/perl5/lib/perl5:$PERL5LIB"
export PATH="/home/holznera/perl5/bin:$PATH"
